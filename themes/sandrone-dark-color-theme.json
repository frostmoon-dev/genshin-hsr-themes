    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#607080",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#d05060",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#80a0d0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes/Types",
            "scope": [
                "entity.name.type",
                "support.class",
                "support.type",
                "storage.type"
            ],
            "settings": {
                "foreground": "#c0a888",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#d098a8"
            }
        },
        {
            "name": "String Escape",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#b898a0"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#d8d0e0"
            }
        },
        {
            "name": "Numbers/Constants",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#98a8c8"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#a098b0"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#9090a8"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#708890"
            }
        },
        {
            "name": "Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#a088a0"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name",
                "meta.tag entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#90a8d0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Selectors",
            "scope": [
                "meta.selector",
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#d0dce8"
            }
        },
        {
            "name": "CSS Properties",
            "scope": [
                "meta.property-name",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#a088a0"
            }
        },
        {
            "name": "CSS Values",
            "scope": [
                "meta.property-value",
                "support.constant.property-value"
            ],
            "settings": {
                "foreground": "#b0a0a8"
            }
        },
        {
            "name": "Markup Heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#d0a0a8",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup Bold",
            "scope": "markup.bold",
            "settings": {
                "foreground": "#d0dce8",
