  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#B090A0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#a05dd4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#d473a4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes/Types",
      "scope": [
        "entity.name.type",
        "support.class",
        "support.type",
        "storage.type"
      ],
      "settings": {
        "foreground": "#8040A0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#5A8A5A"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#8b95db"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#4A3A50"
      }
    },
    {
      "name": "Numbers/Constants",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant"
      ],
      "settings": {
        "foreground": "#8b95db"
      }
    },
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#a05dd4"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#8A7090"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#B090A0"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#d473a4"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#a05dd4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "meta.selector",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#8040A0"
