  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#607080",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#e060a0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#80a0d0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes/Types",
      "scope": [
        "entity.name.type",
        "support.class",
        "support.type",
        "storage.type"
      ],
      "settings": {
        "foreground": "#e0e4e8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#d098b8"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#b898b0"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#d8d0e0"
      }
    },
    {
      "name": "Numbers/Constants",
      "scope": [
        "constant.numeric",
        "constant.character",
