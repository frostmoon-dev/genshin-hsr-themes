      "settings": {
        "foreground": "#a098c0"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#9090a8"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#708898"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#a088b0"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#90ccea",
        "fontStyle": "italic"
      }
