    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#8888a0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#8c4878",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#5868a0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes/Types",
            "scope": [
                "entity.name.type",
                "support.class",
                "support.type",
                "storage.type"
            ],
            "settings": {
                "foreground": "#2a2430",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#684058"
            }
        },
        {
            "name": "String Escape",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#8c4878"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#2a2430"
            }
        },
        {
            "name": "Numbers/Constants",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#5868a0"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#8c4878"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#605060"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#605868"
            }
        },
        {
            "name": "Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#7d3162"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name",
                "meta.tag entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#7487db",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Selectors",
            "scope": [
                "meta.selector",
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#272228"
