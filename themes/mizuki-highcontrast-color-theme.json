    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#7878a0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#e090e8",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Control Keywords",
            "scope": [
                "keyword.control",
                "keyword.control.flow"
            ],
            "settings": {
                "foreground": "#f0a8f0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#d8e8ec",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes/Types",
            "scope": [
                "entity.name.type",
                "support.class",
                "support.type",
                "storage.type"
            ],
            "settings": {
                "foreground": "#88e0f0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#f0c0f0"
            }
        },
        {
            "name": "String Escape",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#f8a8f8"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#f0ecf8"
            }
        },
        {
            "name": "Variable Parameters",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#b8e0f0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Numbers/Constants",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#c8c0f0"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#e090e8"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#b0b0d0"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#a0a0c0"
            }
        },
        {
            "name": "Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#e090e8"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name",
                "meta.tag entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#88e0f0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Selectors",
            "scope": [
                "meta.selector",
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#f0ecf8"
            }
        },
        {
            "name": "CSS Properties",
            "scope": [
                "meta.property-name",
                "support.type.property-name"
