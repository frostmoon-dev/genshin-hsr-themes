  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8C6A78",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#713254",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#5A4A6A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes/Types",
      "scope": [
        "entity.name.type",
        "support.class",
        "support.type",
        "storage.type"
      ],
      "settings": {
        "foreground": "#231C20",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#5A3A4A"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#713254"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#231C20"
      }
    },
    {
      "name": "Numbers/Constants",
      "scope": [
        "constant.numeric",
