    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#6B8E8E",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#790000",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#2E5B63",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes/Types",
            "scope": [
                "entity.name.type",
                "support.class",
                "support.type",
                "storage.type"
            ],
            "settings": {
                "foreground": "#133139",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#1A6B6B"
            }
        },
        {
            "name": "String Escape",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#790000"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#133139"
            }
        },
        {
            "name": "Numbers/Constants",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#790000"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#5A0000"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#2E5B63"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#4A6A6A"
            }
        },
        {
            "name": "Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#790000"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name",
                "meta.tag entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#2E5B63",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Selectors",
            "scope": [
                "meta.selector",
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#133139"
            }
        },
        {
            "name": "CSS Properties",
            "scope": [
                "meta.property-name",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#790000"
            }
        },
        {
            "name": "CSS Values",
            "scope": [
                "meta.property-value",
                "support.constant.property-value"
            ],
            "settings": {
                "foreground": "#1A6B6B"
            }
        },
        {
            "name": "Markup Heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#790000",
                "fontStyle": "bold"
            }
        },
        {
