  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#706860",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#7AACA5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#C8B090",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes/Types",
      "scope": [
        "entity.name.type",
        "support.class",
        "support.type",
        "storage.type"
      ],
      "settings": {
        "foreground": "#E0D8D2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#8AAA8A"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#8ABAB2"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#E0D8D2"
      }
    },
    {
      "name": "Numbers/Constants",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant"
      ],
      "settings": {
        "foreground": "#6F9C97"
      }
    },
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#8ABAB2"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#A0988E"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#908880"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#7AACA5"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#C8B090",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "meta.selector",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#E0D8D2"
