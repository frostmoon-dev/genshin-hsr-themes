    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#6a6890",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#c673c9",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Control Keywords",
            "scope": [
                "keyword.control",
                "keyword.control.flow"
            ],
            "settings": {
                "foreground": "#d890d8",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#c1d0d2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes/Types",
            "scope": [
                "entity.name.type",
                "support.class",
                "support.type",
                "storage.type"
            ],
            "settings": {
                "foreground": "#88c8d8",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#dba8dc"
            }
        },
        {
            "name": "String Escape",
            "scope": "constant.character.escape",
